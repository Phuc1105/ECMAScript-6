{
  "devDependencies": {
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.1"
  },
  "scripts": {
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,html,css,md}":[
      "prettier --single-quote --write",
     "git add"
    ]
  }
  
}
